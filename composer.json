{
    "name": "kisphp/twig-extensions",
    "description": "Kisphp twig extensions",
    "license": "MIT",
    "authors": [
        {
            "name": "Marius",
            "email": "mariusbogdan83@gmail.com",
            "homepage": "http://www.kisphp.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6",
        "twig/twig": "~1.34|<2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7.0",
        "symfony/var-dumper": "~2.3",
        "friendsofphp/php-cs-fixer": "~1.9",
        "symfony/stopwatch": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Kisphp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}
