{
    "name": "kisphp/twig-extensions",
    "description": "Kisphp twig extensions",
    "license": "MIT",
    "authors": [
        {
            "name": "Marius-Bogdan Rizac",
            "email": "mariusbogdan83@gmail.com",
            "homepage": "http://www.kisphp.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0",
        "twig/twig": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "symfony/var-dumper": "^3.0",
        "friendsofphp/php-cs-fixer": "~2.0",
        "phpstan/phpstan": "^0.6.4"
    },
    "autoload": {
        "psr-4": {
            "Kisphp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}
